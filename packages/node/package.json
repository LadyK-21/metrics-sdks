{
  "name": "readmeio",
  "description": "Track your API metrics within ReadMe.",
  "version": "4.0.0",
  "license": "ISC",
  "homepage": "https://readme.com/metrics",
  "bugs": {
    "email": "support@readme.io",
    "url": "https://github.com/readmeio/metrics-sdks/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:readmeio/metrics-sdks.git"
  },
  "keywords": [
    "api-metrics",
    "readme"
  ],
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "content-type": "^1.0.4",
    "find-cache-dir": "^3.3.1",
    "flat-cache": "^2.0.1",
    "lodash": "^4.17.15",
    "node-fetch": "^2.6.0",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@readme/eslint-config": "^3.0.0",
    "body-parser": "^1.19.0",
    "conventional-changelog-cli": "^2.0.31",
    "eslint": "^7.0.0",
    "express": "^4.17.1",
    "is-valid-uuid-v4": "^1.2.0",
    "jest": "^26.0.1",
    "jest-expect-message": "^1.0.2",
    "prettier": "^2.0.1",
    "rimraf": "^3.0.2",
    "socket.io-client": "^2.3.0",
    "supertest": "^4.0.2"
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint && npm run prettier",
    "prettier": "prettier --list-different --write \"./**/**.js\"",
    "release": "conventional-changelog -i CHANGELOG.md -s",
    "test": "jest --coverage"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "<rootDir>/config/"
    ],
    "setupFilesAfterEnv": [
      "jest-expect-message"
    ]
  },
  "prettier": "@readme/eslint-config/prettier"
}
